  {
    "bindings": {
      "alt alt": "file_finder::Toggle",
      "f e": "project_panel::ToggleFocus",
			"alt-n": null

    }
  },
  {
    "context": "ProjectPanel && not_editing",
    "bindings": {
      "H": "project_panel::CollapseAllEntries",
